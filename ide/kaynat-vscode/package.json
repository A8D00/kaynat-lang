{
  "name": "kaynat-lang",
  "displayName": "لغة الكيانات",
  "description": "دعم لغة الكيانات العربية",
  "version": "0.0.1",
  "publisher": "kaynat",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "commands": [
  {
    "command": "kaynat.run",
    "title": "تشغيل ملف كيان"
  }
    ]
    "languages": [
      {
        "id": "kaynat",
        "aliases": ["الكيانات", "kaynat"],
        "extensions": [".كيان"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kaynat",
        "scopeName": "source.kaynat",
        "path": "./syntaxes/kaynat.tmLanguage.json"
      }
    ]
  }
}
"main": "./extension.js",
"activationEvents": ["onCommand:kaynat.run"]

